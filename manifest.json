{
    "manifest_version": 3,
    "name": "Meets+",
    "description": "Additional functionalities to Google Meets",
    "version": "1.0",
    "action": {
        "default_title": "Meets+",
        "default_popup": "./html/popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://meet.google.com/*"],
            "js": ["./js/content.js"]
        }
    ]
}